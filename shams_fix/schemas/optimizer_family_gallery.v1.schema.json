        "properties": {
          "family_id": {
            "type": "integer"
          },
          "n_feasible": {
            "minimum": 0,
            "type": "integer"
          },
          "objective": {
            "type": "object"
          },
          "representatives": {
            "type": "object"
          },
          "worst_hard_margin": {
            "type": "object"
          }
        },
        "required": [
          "family_id",
          "n_feasible",
          "representatives"
        ],
        "type": "object"
      },
      "type": "array"
    },
    "objective_direction": {
      "enum": [
        "min",
        "max",
        ""
      ],
      "type": "string"
    },
    "objective_key": {
      "type": "string"
    },
    "schema": {
      "const": "optimizer_family_gallery.v1"
    }
  },
  "required": [
    "schema",
    "objective_key",
    "objective_direction",
    "families"
  ],
  "title": "optimizer_family_gallery.v1",
  "type": "object"
}